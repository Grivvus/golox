class Doughnut {
  cook() {
    print "Dunk in the fryer.";
    this.finish("sprinkles");
  }

  finish(ingredient) {
    print "Finish with " + ingredient;
  }
}

class Cruller < Doughnut {
  finish(ingredient) {
    // No sprinkles, always icing.
    super.finish("icing");
  }
}

for (var i = 0; i < 10000; i = i + 1) {
    Doughnut().cook();
    Cruller().cook();
    Cruller().finish("whatever");
}
